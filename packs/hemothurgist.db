{"img":"systems/pf2e/icons/features/classes/lightning-reflexes.webp","name":"Lightning Reflexes(Hemo)","system":{"description":{"value":"<p>Your reflexes are lightning fast. Your proficiency rank for Reflex saves increases to expert.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["hb_hemoturge"],"rarity":"common","custom":""},"rules":[{"key":"ActiveEffectLike","mode":"upgrade","path":"system.saves.reflex.rank","value":2}],"slug":"lightning-reflexes","schema":{"version":0.815,"lastMigration":null},"level":{"value":5},"featType":{"value":"classfeature"},"onlyLevel1":false,"maxTakable":1,"actionType":{"value":"passive"},"actions":{"value":null},"prerequisites":{"value":[]},"location":null},"type":"feat","flags":{"core":{"sourceId":"Compendium.pf2e.classfeatures.TUOeATt52P43r5W0"}},"effects":[],"_stats":{"systemId":"pf2e","systemVersion":"4.6.8","coreVersion":"10.291","createdTime":1678127076941,"modifiedTime":1678127241501,"lastModifiedBy":"YUypsikoqTNePUh7"},"folder":null,"sort":0,"ownership":{"default":0,"YUypsikoqTNePUh7":3},"_id":"XYkqE0KbC1fI3p5g"}
